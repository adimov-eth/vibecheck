{
  "name": "vibecheck-api",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts",
    "worker:audio": "bun src/workers/audio.worker.ts",
    "worker:gpt": "bun src/workers/gpt.worker.ts",
    "workers": "bun src/workers/audio-worker.ts & bun src/workers/gpt-worker.ts",
    "clean-queues": "bun src/scripts/clean-queues.ts",
    "dev": "bun --watch src/index.ts",
    "dev:workers": "bun --watch src/workers/index.ts",
    "db:migrate": "bun src/scripts/init-db.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src --ext .ts",
    "check:openai": "bun src/scripts/check-openai-quota.ts",
    "queue:clean": "bun src/scripts/clean-queues.ts clean",
    "queue:clean:audio": "bun src/scripts/clean-queues.ts clean --audio",
    "queue:clean:gpt": "bun src/scripts/clean-queues.ts clean --gpt",
    "queue:drain": "bun src/scripts/clean-queues.ts drain",
    "queue:restart": "bun src/scripts/clean-queues.ts restart",
    "jwt:rotate": "bun src/scripts/jwt-key-rotation-scheduler.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:auth": "bun test src/**/__tests__/*auth*.test.ts src/test/security/*.test.ts",
    "test:unit": "bun test src/**/__tests__/*.test.ts",
    "test:integration": "bun test src/**/__tests__/*.integration.test.ts",
    "test:security": "bun test src/test/security/*.test.ts",
    "build": "echo 'No build required for Bun'",
    "db:migrate:postgres": "bun src/scripts/migrate-to-postgres.ts",
    "db:migrate:postgres:dry": "bun src/scripts/migrate-to-postgres.ts --dry-run",
    "db:migrate:postgres:verify": "bun src/scripts/migrate-to-postgres.ts --verify",
    "db:generate:postgres": "drizzle-kit generate --config drizzle.config.postgres.ts",
    "db:push:postgres": "drizzle-kit push --config drizzle.config.postgres.ts",
    "db:studio:postgres": "drizzle-kit studio --config drizzle.config.postgres.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@faker-js/faker": "^9.8.0",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.13",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "heapdump": "^0.3.15",
    "inspector": "^0.5.0",
    "supertest": "^7.1.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "ws": "^8.18.2"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/jwk-to-pem": "^2.0.3",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.15.4",
    "bullmq": "^5.44.4",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "drizzle-orm": "^0.38.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.0",
    "express-slow-down": "^2.0.0",
    "helmet": "^8.1.0",
    "jose": "^6.0.10",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "2",
    "openai": "^4.89.0",
    "postgres": "^3.4.7",
    "rate-limiter-flexible": "^3.0.0",
    "redis": "^4.7.0",
    "svix": "^1.62.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
